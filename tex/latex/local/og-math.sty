\newcommand{\dletter}{\mathrm{d}}
\newcommand{\dd}{\mathop{}\!\dletter}
\newcommand{\D}{\mathop{}\!\mathrm{D}}

\newcommand{\mdd}[2][]{
\ifthenelse{\equal{#1}{}}
    {\def\temp {\dletter}}
    {\def\temp {\dletter^{{#1}}}}
\mathop{}\!\temp #2}
\newcommand{\mpd}[2][]{
\ifthenelse{\equal{#1}{}}
    {\def\temp {\partial}}
    {\def\temp {\partial^{{#1}}}}
\temp #2}

\newcommand{\dv}[3][]{\frac{\mdd[#1]{#2}}{\mdd{#3}\ifthenelse{\equal{#1}{}}
    {\def\temp {}}
    {\def\temp {^{#1}}}\temp}}
\newcommand{\pdv}[3][]{\frac{\mpd[#1]{#2}}{\mpd{#3}\ifthenelse{\equal{#1}{}}
    {\def\temp {}}
    {\def\temp {^{#1}}}\temp}}

